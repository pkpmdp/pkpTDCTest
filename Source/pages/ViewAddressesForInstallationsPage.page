                   divIdName=divIdName + '</tr>'; 
                   divIdName = divIdName+ '<tr style="background-color:#eeeeee;">';
                       divIdName = divIdName+ '<td colspan=2 style="background-color:#eeeeee;"><b>Customer Name : </b>' + customerName+ '</td>';
                   divIdName = divIdName + '</tr>';
                   divIdName = divIdName+ '<tr style="background-color:#eeeeee;">';
                       divIdName = divIdName+ '<td colspan=2 style="background-color:#eeeeee;"><b>Customer Email : </b>' + custemail+ '</td>';
                   divIdName = divIdName + '</tr>';
               divIdName = divIdName + '</Table>';
               
            }else{
                divIdName = '<Table width="120%" style="background-color:#eeeeee;">';
                   divIdName =divIdName+ '<tr style="background-color:#eeeeee;" width="140%">';
                       divIdName =divIdName+ '<td style="background-color:#eeeeee;"><center><b> Enkel Info </b></center></td>';
                       divIdName =divIdName+ '<td style="background-color:#eeeeee;"><right><a href="javascript:hideObject(\'' + popupId + '\');" style="float: right"><img src="{!$Resource.Nej_icon}"></img></a></right></td>';
