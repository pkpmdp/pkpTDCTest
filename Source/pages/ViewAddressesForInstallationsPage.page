           var divTag = document.getElementById(popupId);
           divTag.style.visibility='visible'; 
           divTag.style.zIndex='150';
            //divTag.style.position='relative';
           var divIdName='';
           if(customerName || custemail){
              
               divIdName = '<Table width="120%" style="background-color:#eeeeee;">';
