    function confirmMail(){
        var locale = '{!localeSetting}';
        var userInput;
        if(locale == 'da_DK'){
            userInput = confirm('Eksporterede adresser sendes til din e-mailadresse i csv-format. Er du sikker?');
        }else{
            userInput = confirm('Exported adresses will be sent to your e-mail in csv-format. Are you sure?');
        }
        return userInput;
    }
</script>

