                </apex:outputPanel>
                <apex:outputPanel rendered="{!ListSize != 1}">
                    <th style="width: 10%;position: relative;"></th>
                </apex:outputPanel>
                <apex:repeat value="{!netInstList}" var="inst">
                    <th><apex:outputText value="{!$Label.Cable_unit_Name} {!inst.Cable_Unit_No__c}"></apex:outputText></th>
                </apex:repeat>
                
            </tr>
