<script type="text/javascript">

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function getPrettyValue() {
    if('{!contact.email}'!='' && '{!contact.Eloqua_ID_Advanced__c}'=='') {   
        //alert('{!contact.email}');    
        return '{!contact.email}';
    } else if ('{!contact.email}'!='' && '{!contact.Eloqua_ID_Advanced__c}'!='' && !hasElqRegistered) {
        alert('right choice');
        return '{!contact.email}';
    } else {
      // alert('WEN'+'{!contact.email}'+'h'+'{!contact.Eloqua_ID_Advanced__c}');    
        return '{!contact.Eloqua_ID_Advanced__c}';
    }   
}


var splitUrl = document.URL.split("/");
var initialPage = true;
var hasValue = false;
var elqRegisterEmail;
